<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <section class="content-header">
    <div class="container-fluid">
      <div class="row mb-2">
        <div class="col-sm-6">
          <h1>Thêm món ăn</h1>
        </div>
        <div class="col-sm-6">
          <ol class="breadcrumb float-sm-right">
            <li class="breadcrumb-item"><a href="#">Trang quản lý</a></li>
            <li class="breadcrumb-item active">Thêm món ăn</li>
          </ol>
        </div>
      </div>
    </div><!-- /.container-fluid -->
  </section>

  <!-- Main content -->
  <section class="content">
    <div class="container-fluid">
      <div class="row">
        <!-- left column -->
        <div class="col-md-6">
          <!-- general form elements -->
          <div class="card card-primary">
            
            <!-- /.card-header -->
            <!-- form start -->
            <form role="form" action="approval-restaurant/<%= restaurant._id %>" method="post">
              <div class="card-body">
                <div class="form-group">
                  <label>Tên chủ quán</label>
                  <input type="text" class="form-control" value="<%= restaurant.manager_name %>" disabled >
                </div>
                <div class="form-group">
                  <label>Tên quán</label>
                  <input type="text" class="form-control" value="<%= restaurant.name %>" disabled>
                </div>
                <div class="form-group">
                  <label>Số điện thoại</label>
                  <input type="text" class="form-control" value="<%= restaurant.number_phone %>" disabled>
                </div>
                <div class="form-group">
                  <label>Địa chỉ</label>
                  <input type="text" class="form-control" value="<%= restaurant.address %>" disabled>
                </div>
              </div>
              <!-- /.card-body -->
              <div class="card-footer">
                <button type="submit" class="btn btn-primary">Duyệt</button>
              </div>
            </form>
          </div>
          <!-- /.card -->

          


        </div>
        
      </div>
      <!-- /.row -->
    </div><!-- /.container-fluid -->
  </section>
  <!-- /.content -->
</div>